{
  "name": "chevereto/app",
  "description": "Chevereto app",
  "type": "library",
  "license": "MIT",
  "authors": [{
    "name": "Rodolfo Berrios",
    "email": "inbox@rodolfoberrios.com"
  }],
  "repositories": [{
    "type": "path",
    "url": "./Chevereto-Chevere",
    "options": {
      "symlink": true
    }
  }],
  "autoload": {
    "psr-4": {
      "App\\": "app/src/"
    }
  },
  "require": {
    "chevereto/chevere": "@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "config": {
    "optimize-autoloader": true
  }
}